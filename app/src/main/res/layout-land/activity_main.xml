            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/mx_2"
            android:layout_marginEnd="@dimen/mx_2"
            android:layout_weight="1"
            android:backgroundTint="@color/numberButtonsBackground"
