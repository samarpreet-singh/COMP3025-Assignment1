            android:fontFamily="@font/fa_v4compatibility"
            android:minWidth="@dimen/minWidthButtonLandscape"
            android:minHeight="@dimen/minHeightButtonLandscape"
            android:tag="1"
            android:text="@string/brackets"
            android:textColor="@color/black"
            android:textSize="@dimen/buttonIconSizeLandscape" />

